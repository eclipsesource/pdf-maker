{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "outDir": "dist",
    "lib": ["ES2019", "DOM"],
    "moduleResolution": "Node",
    "sourceMap": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "strict": true
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.js"]
}
