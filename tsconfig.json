{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "outDir": "dist",
    "lib": ["ES2019"],
    "moduleResolution": "Node",
    "sourceMap": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.js"
  ]
}
